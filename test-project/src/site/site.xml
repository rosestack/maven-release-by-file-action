<?xml version="1.0" encoding="UTF-8"?>
<site xmlns="http://maven.apache.org/SITE/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SITE/2.0.0 https://maven.apache.org/xsd/site-2.0.0.xsd">
  <custom>
    <fluidoSkin>
      <copyrightClass>pull-right</copyrightClass>
      <gitHub>
        <projectId>${project.organization.name}/${project.artifactId}</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>gray</ribbonColor>
      </gitHub>
      <facebookLike>
        <shareButton>true</shareButton>
      </facebookLike>
      <twitter>
        <user>ichensoul</user>
        <showUser>true</showUser>
      </twitter>
    </fluidoSkin>
  </custom>

  <publishDate position="right"/>
  <version position="right"/>

  <bannerLeft name="${project.name}"/>

  <body>
    <breadcrumbs>
      <item name="Home" href="index.html"/>
    </breadcrumbs>

    <menu name="Overview">
      <item name="Introduction" href="index.html"/>
      <item name="Private key preparation" href="key-prepare.html"/>
    </menu>

    <menu ref="reports" inherit="bottom"/>
  </body>

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>2.1.0</version>
  </skin>
</site>
